{
  "name": "renshift-sol",
  "version": "0.1.0",
  "scripts": {
    "generate": "truffle compile && typechain --target truffle './build/**/*.json'",
    "coverage": "yarn run generate && solidity-coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test": "yarn run generate && truffle test",
    "bindings:ts": "truffle compile && typechain --target web3-1.0.0 './build/**/*.json'",
    "bindings:go": "solc --allow-paths . --combined-json bin,abi,userdoc,devdoc,metadata $(find contracts -type f -name '*.sol') | abigen -pkg bindings --out bindings.go --abi -",
    "deploy": "truffle migrate --skipDryRun --network $NETWORK",
    "verify": "truffle run verify --network $NETWORK",
    "prepare": "patch-package"
  },
  "dependencies": {
    "darknode-sol": "github:renproject/darknode-sol#aba92b64670b3f16d9168f46eea729cc88f31727",
    "openzeppelin-solidity": "2.3.0",
    "truffle-typings": "^1.0.6",
    "ts-node": "^8.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/elliptic": "^6.4.8",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.7",
    "any-promise": "^1.3.0",
    "bignumber.js": "^9.0.0",
    "bn.js": "^4.11.8",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^3.0.0",
    "coveralls": "^3.0.4",
    "dotenv": "^8.0.0",
    "elliptic": "^6.4.1",
    "eth-gas-reporter": "^0.2.0",
    "ethereumjs-util": "^6.1.0",
    "ganache-cli": "^6.4.3",
    "patch-package": "^6.1.2",
    "postinstall-postinstall": "^2.0.0",
    "sol-merger": "^0.1.4",
    "solc": "0.5.8",
    "solidity-coverage": "0.6.0-beta.5",
    "truffle": "^5.0.21",
    "truffle-hdwallet-provider": "^1.0.10",
    "truffle-plugin-verify": "^0.2.1",
    "ts-generator": "^0.0.8",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "typechain": "^0.3.14",
    "typescript": "^3.5.1",
    "web3": "^1.0.0-beta.35"
  },
  "resolutions": {
    "solc": "0.5.8"
  },
  "author": "Ren",
  "license": "GPL-3.0-or-later"
}

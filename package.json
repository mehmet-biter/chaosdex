{
  "name": "shifter-sol",
  "version": "0.1.0",
  "config": {
    "truffleBuildPath": "client/src/contracts"
  },
  "scripts": {
    "generate": "truffle compile && yarn run typings --target truffle",
    "typings": "echo $npm_package_config_truffleBuildPath'/**/*.json' && typechain $npm_package_config_truffleBuildPath'/**/*.json'",
    "coverage": "yarn run generate && solidity-coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test": "yarn run generate && truffle test",
    "bindings:ts": "truffle compile && yarn run typings --target web3-1.0.0 --outDir './client/src/lib/contracts'",
    "bindings:go": "solc --allow-paths . --combined-json bin,abi,userdoc,devdoc,metadata $(find contracts -type f -name '*.sol') | abigen -pkg bindings --out bindings.go --abi -",
    "deploy": "truffle migrate --skipDryRun --network $NETWORK",
    "verify": "truffle run verify --network $NETWORK",
    "prepare": "patch-package"
  },
  "dependencies": {
    "darknode-sol": "https://github.com/renproject/darknode-sol#shifter",
    "openzeppelin-solidity": "2.3.0",
    "truffle-typings": "^1.0.8",
    "ts-node": "^8.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/elliptic": "^6.4.9",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.8",
    "any-promise": "^1.3.0",
    "bignumber.js": "^9.0.0",
    "bn.js": "^4.11.8",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^3.0.0",
    "coveralls": "^3.0.4",
    "dotenv": "^8.0.0",
    "elliptic": "^6.4.1",
    "eth-gas-reporter": "^0.2.1",
    "ethereumjs-util": "^6.1.0",
    "ganache-cli": "^6.4.4",
    "patch-package": "^6.1.2",
    "postinstall-postinstall": "^2.0.0",
    "sol-merger": "^0.1.4",
    "solc": "0.5.9",
    "solidity-coverage": "0.6.0-beta.5",
    "truffle": "^5.0.21",
    "truffle-hdwallet-provider": "^1.0.10",
    "truffle-plugin-verify": "^0.2.2",
    "ts-generator": "^0.0.8",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "typechain": "^0.3.17",
    "typescript": "^3.5.2",
    "web3": "^1.0.0-beta.35"
  },
  "resolutions": {
    "solc": "0.5.9"
  },
  "author": "Ren",
  "license": "GPL-3.0-or-later"
}

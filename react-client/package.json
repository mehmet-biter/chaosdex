    "devDependencies": {
        "gh-pages": "^2.2.0",
        "node-sass": "^4.13.1",
        "npm-check-updates": "^4.0.1",
        "react-scripts": "^3.3.1",
        "ts-unused-exports": "^6.0.0",
        "tslint": "^6.0.0",
        "tslint-microsoft-contrib": "^6.2.0",
        "tslint-react": "^4.2.0",
        "typescript": "^3.7.5"
    },
    "browserslist": [
        ">0.2%",

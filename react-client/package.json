{
    "name": "renvm-demo",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://chaosdex.herokuapp.com",
    "scripts": {
        "lint:unused-exports": "ts-unused-exports ./tsconfig.json",
        "lint:outdated-imports": "ncu",
        "lint:ts": "tslint --project ./ -t stylish --quiet",
        "lint:extra": "$npm_execpath run --silent lint:unused-exports; $npm_execpath run --silent lint:outdated-imports",
        "lint:all": "$npm_execpath run --silent lint:ts && $npm_execpath run --silent lint:extra",
        "lint": "$npm_execpath run --silent lint:ts",
        "unused-exports": "ts-unused-exports ./tsconfig.json src/**/*.ts src/**/*.tsx",
        "start": "react-scripts start",
        "build": "NODE_OPTIONS=--max_old_space_size=4096 REACT_APP_SOURCE_VERSION=$SOURCE_VERSION react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "$npm_execpath run build",
        "deploy": "gh-pages -d build"
    },
    "dependencies": {
        "@renproject/fonts": "^1.0.1",
        "@renproject/react-components": "^1.0.35",
        "@renproject/ren": "git+https://github.com/renproject/renvm-sdk-js.git#06757c4779106320108a027fbb661f6d453cba08",
        "@sentry/browser": "^5.7.1",
        "@sentry/core": "^5.7.1",
        "@types/bchaddrjs": "^0.4.0",
        "@types/bn.js": "^4.11.5",
        "@types/cashaddrjs": "^0.3.0",
        "@types/node": "^12.12.3",
        "@types/qrcode.react": "^0.9.0",
        "@types/qs": "^6.5.3",
        "@types/react": "^16.9.11",
        "@types/react-circular-progressbar": "^1.1.0",
        "@types/react-copy-to-clipboard": "^4.3.0",
        "@types/react-dom": "^16.9.3",
        "@types/react-router-dom": "^5.1.1",
        "axios": "^0.19.0",
        "bchaddrjs": "^0.4.4",
        "bignumber.js": "^9.0.0",
        "bn.js": "^5.0.0",
        "bootstrap": "^4.3.1",
        "cashaddrjs": "^0.3.8",
        "history": "^4.10.1",
        "immutable": "^4.0.0-rc.12",
        "localforage": "^1.7.3",
        "moment": "^2.24.0",
        "qrcode.react": "^0.9.3",
        "qs": "^6.9.0",
        "react": "^16.11.0",
        "react-bootstrap": "^1.0.0-beta.9",
        "react-circular-progressbar": "^2.0.3",
        "react-copy-to-clipboard": "^5.0.1",
        "react-dom": "^16.11.0",
        "react-router-dom": "^5.1.2",
        "unstated": "^2.1.1",
        "unstated-persist": "^0.0.4",
        "use-persisted-state": "^0.3.0",
        "wallet-address-validator": "^0.2.4",
        "web3": "^2.0.0-alpha.1"
    },
    "devDependencies": {
        "gh-pages": "^2.1.1",
        "ncu": "^0.2.1",
        "node-sass": "^4.13.0",
        "npm-check-updates": "^3.1.26",
        "react-scripts": "^3.2.0",
        "ts-unused-exports": "^3.0.3",
        "tslint": "^5.20.0",
        "tslint-microsoft-contrib": "^6.2.0",
        "tslint-react": "^4.1.0",
        "typescript": "^3.6.4"
    },
    "resolutions": {
        "scrypt": "https://github.com/ren-forks/node-scrypt"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
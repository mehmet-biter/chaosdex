        "react-copy-to-clipboard": "^5.0.2",
        "react-dom": "^16.12.0",
        "react-router-dom": "^5.1.2",
        "react-super-responsive-table": "^5.1.1",
        "recharts": "^1.8.5",
        "svgo": "1.3.2",

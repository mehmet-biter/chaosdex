{
  "name": "renvm-demo",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://renproject.github.io/renvm-demo",
  "scripts": {
    "lint:unused-exports": "ts-unused-exports ./tsconfig.json",
    "lint:outdated-imports": "ncu",
    "lint:ts": "tslint --project ./ -t stylish --quiet",
    "lint:extra": "$npm_execpath run --silent lint:unused-exports; $npm_execpath run --silent lint:outdated-imports",
    "lint:all": "$npm_execpath run --silent lint:ts && $npm_execpath run --silent lint:extra",
    "lint": "$npm_execpath run --silent lint:ts",
    "unused-exports": "ts-unused-exports ./tsconfig.json src/**/*.ts src/**/*.tsx",
    "start": "REACT_APP_NETWORK=$NETWORK react-scripts start",
    "build": "REACT_APP_NETWORK=$NETWORK NODE_OPTIONS=--max_old_space_size=4096 REACT_APP_SOURCE_VERSION=$SOURCE_VERSION react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "$npm_execpath run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@renproject/fonts": "^1.0.1",
    "@renproject/react-components": "1.0.30",
    "@renproject/ren": "0.1.8",
    "@sentry/browser": "^5.6.1",
    "@sentry/core": "^5.6.1",
    "@types/bn.js": "^4.11.5",
    "@types/bs58": "^4.0.0",
    "@types/node": "^12.7.2",
    "@types/qrcode.react": "^0.8.2",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.9.1",
    "@types/react-circular-progressbar": "^1.0.2",
    "@types/react-copy-to-clipboard": "^4.2.6",
    "@types/react-dom": "^16.8.5",
    "@types/react-router-dom": "^4.3.4",
    "@types/throttle-debounce": "^1.1.1",
    "axios": "^0.19.0",
    "bignumber.js": "^9.0.0",
    "bn.js": "^5.0.0",
    "bootstrap": "^4.3.1",
    "bs58": "^4.0.1",
    "history": "^4.9.0",
    "immutable": "^4.0.0-rc.12",
    "localforage": "^1.7.3",
    "moment": "^2.24.0",
    "qrcode.react": "^0.9.3",
    "qs": "^6.7.0",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-circular-progressbar": "^2.0.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "throttle-debounce": "^2.1.0",
    "unstated": "^2.1.1",
    "unstated-persist": "^0.0.4",
    "use-persisted-state": "^0.3.0",
    "wallet-address-validator": "^0.2.4",
    "web3": "^2.0.0-alpha.1"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "ncu": "^0.2.1",
    "node-sass": "^4.12.0",
    "npm-check-updates": "^3.1.21",
    "react-scripts": "^3.1.1",
    "ts-unused-exports": "^2.0.11",
    "tslint": "^5.18.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3"
  },
  "resolutions": {
    "scrypt": "https://github.com/ren-forks/node-scrypt"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}

        "@sentry/browser": "^5.12.1",
        "@sentry/core": "^5.12.0",
        "@sentry/integrations": "^5.12.0",
        "@types/bchaddrjs": "^0.4.0",
        "@types/bn.js": "^4.11.6",
        "@types/node": "^13.7.0",
        "@types/qrcode.react": "^1.0.0",
        "@types/qs": "^6.9.0",
        "@types/react": "^16.9.19",
        "@types/react-circular-progressbar": "^1.1.0",
        "@types/react-copy-to-clipboard": "^4.3.0",
        "@types/react-dom": "^16.9.5",
        "@types/react-router-dom": "^5.1.3",
        "@types/recharts": "^1.8.5",
        "axios": "^0.19.2",
        "bchaddrjs": "^0.4.5",
        "bignumber.js": "^9.0.0",
        "bn.js": "^5.1.1",
        "bootstrap": "^4.4.1",
        "history": "^4.10.1",
        "immutable": "^4.0.0-rc.12",
        "localforage": "^1.7.3",
        "moment": "^2.24.0",
        "qrcode.react": "^1.0.0",
        "qs": "^6.9.1",
